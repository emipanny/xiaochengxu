<view class="edit">
  <view class="content">
    <view class="text">编号：</view>
    <text>{{info.goodsNo}}</text>
  </view>
  <view class="content">
    <view class="text">名称：</view>
    <text>{{info.title}}</text>
  </view>
  <view class="content">
    <view class="text">数量：</view>
    <text>{{info.num}}</text>
  </view>
  <view class="content">
    <view class="text">实际用量：</view>
    <text>{{info.consumption}}</text>
  </view>
  <view class="content">
    <view class="text">单位：</view>
    <text>{{info.unit}}</text>
  </view>
  <view class="content">
    <view class="text">开工日期：</view>
    <text>{{info.start_time}}</text>
  </view>
  <view class="content">
    <view class="text">结束时间：</view>
    <text>{{info.end_time}}</text>
  </view>
  <view class="content">
    <view class="text">监理交底：</view>
    <text>{{info.supervisor_start}}</text>
  </view>
  <view class="content">
    <view class="text">完成时间：</view>
    <text>{{info.supervisor_start_finish ? info.supervisor_start_finish : "未完成" }}</text>
  </view>
  <view class="content">
    <view class="text">监理验收：</view>
    <text>{{info.supervisor_end}}</text>
  </view>
  <view class="content">
    <view class="text">完成时间：</view>
    <text>{{info.supervisor_end_finish ? info.supervisor_end_finish : "未完成"}}</text>
  </view>
  <view class="content">
    <view class="text">设计交底：</view>
    <text>{{info.designer_start}}</text>
  </view>
  <view class="content">
    <view class="text">完成时间：</view>
    <text>{{info.designer_start_finish ? info.designer_start_finish : "未完成" }}</text>
  </view>
  <view class="content">
    <view class="text">设计验收：</view>
    <text>{{info.designer_end}}</text>
  </view>
  <view class="content">
    <view class="text">完成时间：</view>
    <text>{{info.designer_end_finish ? info.designer_end_finish : "未完成" }}</text>
  </view>
</view>
